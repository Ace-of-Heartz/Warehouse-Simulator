            <ui:Toggle label="Highlight Robots" />
        </ui:RadioButtonGroup>
        <ui:Foldout text="Config Information" name="GlobalInfos" style="font-size: 18px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0);">
            <ui:TextField picking-mode="Ignore" label="Text Field" value="filler text" name="RobotsAmount" />
            <ui:TextField picking-mode="Ignore" label="Text Field" value="filler text" name="GoalsAmount" />
            <ui:TextField picking-mode="Ignore" label="Text Field" value="filler text" name="GoalsRemaining" />
        </ui:Foldout>
    </ui:VisualElement>
</ui:UXML>
